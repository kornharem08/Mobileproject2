    <icon density="ldpi" src="res/icon/android/icons8-scarlet-shopping-basket-36.png" />
    <icon density="mdpi" src="res/icon/android/icons8-scarlet-shopping-basket-48.png" />
    <icon density="hdpi" src="res/icon/android/icons8-scarlet-shopping-basket-72.png" />
    <icon density="xhdpi" src="res/icon/android/icons8-scarlet-shopping-basket-96.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidLaunchMode" value="singleTask" />
